{
  "name": "vue-symptom-checker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:js": "eslint --ext .js,.vue .",
    "lint-autofix:js": "eslint --ext .js,.vue . --fix",
    "lint:css": "stylelint ./**/*.css ./**/*.scss ./**/*.vue",
    "lint-autofix:css": "stylelint ./**/*.css ./**/*.scss ./**/*.vue --fix"
  },
  "proxy": "http://127.0.0.1:3000",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "all",
    "jsxBracketSameLine": true,
    "arrowParens": "always",
    "useTabs": false
  },
  "dependencies": {
    "axios": "0.19.0",
    "bootstrap": "4.5.0",
    "bootstrap-vue": "2.14.0",
    "core-js": "2.6.5",
    "lodash": "4.17.19",
    "vue": "2.6.10",
    "vue-awesome": "3.5.2",
    "vue-axios": "2.1.5",
    "vue-cookies": "1.5.13",
    "vuex": "3.1.1"
  },
  "devDependencies": {
    "@infermedica/eslint-config-vue": "1.0.3",
    "@infermedica/stylelint-config": "1.1.0",
    "@vue/cli-plugin-babel": "3.7.0",
    "@vue/cli-plugin-eslint": "3.7.0",
    "@vue/cli-service": "3.8.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "5.2.2",
    "node-sass": "4.14.1",
    "sass-loader": "7.1.0",
    "vue-cli-plugin-webpack-bundle-analyzer": "1.3.0",
    "vue-router": "3.1.6",
    "vue-template-compiler": "2.6.10"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": "@infermedica/eslint-config-vue",
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".vue",
            ".js"
          ]
        }
      }
    }
  },
  "stylelint": {
    "extends": "@infermedica/stylelint-config"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
